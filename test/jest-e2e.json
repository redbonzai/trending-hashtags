{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "@app/logger/(.*)": "<rootDir>/../libs/logger/src/$1",
    "@app/logger": "<rootDir>/../libs/logger/src",
    "@app/database/(.*)": "<rootDir>/../src/database/$1",
    "@app/database": "<rootDir>/../src/database",
    "@app/interfaces/(.*)": "<rootDir>/../src/interfaces/$1",
    "@app/interfaces": "<rootDir>/../src/interfaces",
    "@app/queues/(.*)": "<rootDir>/../src/queues/$1",
    "@app/queues": "<rootDir>/../src/queues",
    "@app/redis/(.*)": "<rootDir>/../src/redis/$1",
    "@app/redis": "<rootDir>/../src/redis",
    "@app/trending/(.*)": "<rootDir>/../src/trending/$1",
    "@app/trending": "<rootDir>/../src/trending",
    "@app/tweet/(.*)": "<rootDir>/../src/tweet/$1",
    "@app/tweet": "<rootDir>/../src/tweet"
  }
}